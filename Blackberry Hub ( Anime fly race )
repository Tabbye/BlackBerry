key = "Test"
if key == "Test" then
getgenv().AutoCollect = false
getgenv().AutoJump = false

--geting th ui
local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()


function aCollect() --name of this function
    while getgenv().AutoCollect == true do wait() --says while getgenv().autoclick == true, loop and fire our click remote
        -- Script generated by SimpleSpy - fixed version for exploits other than synapse & krnl made by stummy#002 credits to exx#9394 for originally creating the script.

local args = {
    [1] = {
        [1] = 1,
        [2] = 1,
        [3] = 1,
        [4] = 0
    }
}

game:GetService("ReplicatedStorage").Remotes.CollectOrb:FireServer(unpack(args))

    end
end


local Window = Library.CreateLib("Blackberry Hub", "Midnight") --UI title/Window and themes

local Tab = Window:NewTab("Autofarm") --creating the tab name

local Section = Tab:NewSection("Auto Collect") -- creating the section name in the tab

Section:NewToggle("Toggle", "This will auto collect for you.", function(bool) --bool is basically toggle on
    getgenv().AutoCollect = bool 
        if bool then --says if it = bool then fire our aClick() function we made up top
        aCollect()
    end
end)
local Section = Tab:NewSection("Auto Jump")
function aJump() --name of this function
    while getgenv().AutoJump == true do wait() --says while getgenv().autoclick == true, loop and fire our click remote
        -- Script generated by SimpleSpy - fixed version for exploits other than synapse & krnl made by stummy#002 credits to exx#9394 for originally creating the script.

game:GetService("ReplicatedStorage").Remotes.JumpFinished:InvokeServer()

    end
end
Section:NewToggle("Toggle", "This will auto jump for you.", function(bool) --bool is basically toggle on
    getgenv().AutoJump = bool 
        if bool then --says if it = bool then fire our aClick() function we made up top
        aJump()
    end
end)
else end
