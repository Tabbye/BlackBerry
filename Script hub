local Rayfield = loadstring(game:HttpGet('https://raw.githubusercontent.com/shlexware/Rayfield/main/source'))()

local Window = Rayfield:CreateWindow({
    Name = "Blackberry Hub",
    LoadingTitle = "Blackberry Hub",
    LoadingSubtitle = "by Conan",

    KeySystem = true, -- Set this to true to use our key system
    KeySettings = {
       Title = "Blackberry Hub",
       Subtitle = "Blackberry Hub Key",
       Note = "Join for key, copied (https://discord.gg/E9FvpcsMZr)",
       FileName = "",
       SaveKey = false,
       GrabKeyFromSite = true, -- If this is true, set Key below to the RAW site you would like Rayfield to get the key from
       Key = "https://pastebin.com/raw/TLyAKFZ8"
    }
 })
 setclipboard("https://discord.gg/E9FvpcsMZr")

 local Tab = Window:CreateTab("Script Hub") -- Title, Image
 local Section = Tab:CreateSection("Game Detector")

 local Button = Tab:CreateButton({
    Name = "CLICK ME TO EXECUTE SCRIPT",
    Callback = function()
        loadstring(game:HttpGet("https://raw.githubusercontent.com/Tabbye/BlackBerry/main/Game%20List"))()
    end,
 })
