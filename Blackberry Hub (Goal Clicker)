    while getgenv().autoEgg2 == true do wait() --says while getgenv().autoclick == true, loop and fire our click remote

-- Script generated by SimpleSpy - fixed version for exploits other than synapse & krnl made by stummy#002 credits to exx#9394 for originally creating the script.

local args = {
    [1] = "Uncommon",
    [2] = "Single"
}

game:GetService("ReplicatedStorage").RemoteEvents.EggOpened:InvokeServer(unpack(args))




    end
end

