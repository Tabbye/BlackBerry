getgenv().autoClick = false
getgenv().autoKick = false
getgenv().autoEgg = false
getgenv().autoEgg2 = false
getgenv().autoEgg3 = false
getgenv().autoEgg4 = false
getgenv().autoEgg5 = false
getgenv().autoEgg6 = false

--geting th ui
local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()


function aClick() --name of this function
    while getgenv().autoClick == true do wait() --says while getgenv().autoclick == true, loop and fire our click remote
game:GetService("Players").LocalPlayer.PlayerGui.RemoteEvents.Click:FireServer()

    end
end

function aKick() --name of this function
    while getgenv().autoKick == true do wait() --says while getgenv().autoclick == true, loop and fire our click remote
game:GetService("Players").LocalPlayer.PlayerGui.RemoteEvents.Start:FireServer()


    end
end

local Window = Library.CreateLib("Blackberry Hub", "Midnight") --UI title/Window and themes

local Tab = Window:NewTab("Autofarm") --creating the tab name

local Section = Tab:NewSection("Auto click") -- creating the section name in the tab

Section:NewToggle("Toggle", "This will auto click for you.", function(bool) --bool is basically toggle on
    getgenv().autoClick = bool 
        if bool then --says if it = bool then fire our aClick() function we made up top
        aClick()
    end
end)
local Section = Tab:NewSection("Auto Kick") -- creating the section name in the tab

Section:NewToggle("Toggle", "This will auto kick for you.", function(bool) --bool is basically toggle on
    getgenv().autoKick = bool 
        if bool then --says if it = bool then fire our aClick() function we made up top
        aKick()
    end
end)

local Tab = Window:NewTab("Eggs")
local Section = Tab:NewSection("Open Egg ( No egg Animation + Must be near egg )")

function aEgg() --name of this function
    while getgenv().autoEgg == true do wait() --says while getgenv().autoclick == true, loop and fire our click remote

local args = {
    [1] = "Common",
    [2] = "Single"
}

game:GetService("ReplicatedStorage").RemoteEvents.EggOpened:InvokeServer(unpack(args))



    end
end

Section:NewToggle("Common Egg", "This will auto Hatch Eggs for you.", function(bool) --bool is basically toggle on
    getgenv().autoEgg = bool 
        if bool then --says if it = bool then fire our aClick() function we made up top
        aEgg()
    end
end)
